                        "to": [{"key_code": "8"}]
                    },
                    {
                        "type": "basic",
                        "from": {"key_code": "9", "modifiers": {"mandatory": ["shift"], "optional": ["caps_lock"]}},
                        "to": [{"key_code": "9"}]
                    }
                ]
        }
    ]
}
